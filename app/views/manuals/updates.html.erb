<% content_for :extra_headers do %>
  <meta name='description' content="List of updates to '<%= presented_manual.full_title %>'." />
<% end %>
<% content_for :title, 'Updates - ' + presented_manual.full_title %>
<%= render 'header', presented_manual: presented_manual %>
<% if presented_manual.beta? %>
  <%= render partial: 'govuk_component/beta_label' %>
<% end %>
<%=
  render(
    'manual_breadcrumbs',
    presented_manual: presented_manual
  )
%>
<%= render 'manual_updates', presented_manual: presented_manual %>
